generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Tip {
  id        Int      @id @default(autoincrement())
  image     String
  title     String
  short     String
  full      String
  category  String
  level     String
  place     String?
  createdAt DateTime @default(now())
}